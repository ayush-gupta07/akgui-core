{
  "buildCommand": "./build.sh",
  "outputDirectory": "playground/dist",
  "installCommand": "echo 'Dependencies will be installed by build script'",
  "framework": null,
  "env": {
    "NODE_VERSION": "18"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true
}
